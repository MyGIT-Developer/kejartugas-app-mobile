{
  "expo": {
    "name": "Kejar Tugas",
    "slug": "kejar-tugas",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/kt-512.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/kt-512.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/images/kt-512.png",
      "color": "#0E509E",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "New Notification"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.innovation.kejartugas",
      "jsEngine": "hermes",
      "infoPlist": {
        "NSCameraUsageDescription": "Aplikasi ini memerlukan akses kamera untuk mengambil foto.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to location when open.",
        "NSLocationAlwaysUsageDescription": "This app needs access to location when in the background.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to location when open and in the background.",
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/kt-512.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.innovation.kejartugas",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "RECORD_AUDIO",
        "READ_CONTACTS",
        "WRITE_CONTACTS",
        "CALL_PHONE",
        "SEND_SMS",
        "READ_PHONE_STATE",
        "ACCESS_NETWORK_STATE",
        "INTERNET",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "SCHEDULE_EXACT_ALARM",
        "USE_EXACT_ALARM"
      ]
    },
    "plugins": [
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/kt-512.png",
          "color": "#0E509E",
          "mode": "production"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b1891ad1-b0f4-4ef4-8e81-7156592a713d"
      }
    }
  }
}
