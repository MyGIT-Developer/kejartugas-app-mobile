{
    "expo": {
        "name": "Kejar Tugas",
        "slug": "kejar-tugas",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/Meotrik PM Logo.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/images/kt_splash.png"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.innovation.kejartugas"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/Meotrik PM Logo.png",
                "backgroundColor": "#FFFFFF"
            },
            "package": "com.innovation.kejartugas",
            "permissions": [
                "CAMERA",
                "READ_EXTERNAL_STORAGE",
                "WRITE_EXTERNAL_STORAGE",
                "ACCESS_FINE_LOCATION",
                "ACCESS_COARSE_LOCATION",
                "RECORD_AUDIO",
                "READ_CONTACTS",
                "WRITE_CONTACTS",
                "CALL_PHONE",
                "SEND_SMS",
                "READ_PHONE_STATE",
                "ACCESS_NETWORK_STATE",
                "INTERNET"
            ]
        },
        "web": {
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": ["expo-font"],
        "extra": {
            "eas": {
                "projectId": "e1ce8215-b7b5-4f6e-93e9-395c28377593"
            }
        }
    }
}
